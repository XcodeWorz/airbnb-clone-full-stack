{
  "name": "@airbnb-clone/server",
  "version": "1.0.0",
  "description": "Airbnb clone Server",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=develop nodemon src/index.js --exec babel-node",
    "build": "NODE_ENV=production babel lib -d dist",
    "serve": "node dist/index.js",
    "test": "jest",
    "start-test": "NODE_ENV=test babel-node src/index.js",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=8"
  },
  "author": "Laura Coalla",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "graphql-yoga": "^1.16.2",
    "jest": "^23.6.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.1",
    "nodemon": "^1.18.4",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@airbnb-clone/common": "1.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "faker": "^4.1.0"
  }
