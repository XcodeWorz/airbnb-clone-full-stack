{
    "name": "@airbnb/server",
    "version": "1.0.0",
    "description": "Airbnb clone Server",
    "main": "app.js",
    "scripts": {
        "start": "NODE_ENV=develop nodemon src/index.js --exec babel-node",
        "build": "NODE_ENV=production babel lib -d dist",
        "serve": "node dist/index.js",
        "test": "jest",
        "start-test": "NODE_ENV=test babel-node src/index.js",
        "test:watch": "jest --watch"
    },
    "engines": {
        "node": ">=8"
    },
    "author": "Laura Coalla",
    "license": "ISC",
    "dependencies": {
        "graphql-yoga": "^1.16.2",
        "mongoose": "^5.3.1",
        "nodemon": "^1.18.4"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-stage-2": "^6.24.1",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.14.0"
    }
}
