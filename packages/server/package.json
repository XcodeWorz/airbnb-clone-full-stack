{
  "name": "@airbnb-clone/server",
  "version": "1.0.0",
  "description": "Airbnb clone Server",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon src/index.js --exec babel-node",
    "clean": "rm -rf dist && mkdir dist",
    "build-babel": "babel -d ./dist ./src",
    "build": "npm run clean && npm run build-babel",
    "serve": "node dist/index.js",
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles --verbose --runInBand",
    "start-test": "NODE_ENV=test nodemon src/index.js --exec babel-node",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=8"
  },
  "author": "Laura Coalla",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "connect-redis": "^3.4.0",
    "cookie-parser": "^1.4.3",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "express-rate-limit": "^3.3.0",
    "express-session": "^1.15.6",
    "graphql-date": "^1.0.3",
    "graphql-yoga": "^1.16.2",
    "ioredis": "^4.2.0",
    "mongoose": "^5.3.1",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prettier": "^1.14.3",
    "rate-limit-redis": "^1.5.0",
    "redis": "^2.8.0",
    "uuid": "^3.3.2",
    "yup": "^0.26.6",
    "@airbnb-clone/common": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-watch": "^2.0.7",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "faker": "^4.1.0",
    "fetch-cookie": "^0.7.2",
    "graphql-request": "^1.8.2",
    "jest": "^23.6.0",
    "node-fetch": "^2.2.1",
    "nodemon": "^1.18.4",
    "regenerator-runtime": "^0.12.1",
    "request-promise": "^4.2.2"
  }
}
